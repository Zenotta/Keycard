  "license": "MIT",
  "scripts": {
    "start": "node ./index.js",
    "build": "rimraf dist && esbuild ./index.js --bundle --platform=node --target=node16.14.2 --outfile=dist/index.js",
    "build:artifacts": "npm run build && npx pkg -t linux,macos,win . --out-path dist"
