  "dependencies": {
    "@zenotta/zenotta-js": "^1.2.4",
    "chalk": "^4.1.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "redis": "^4.1.1"
  },
  "devDependencies": {
    "esbuild": "^0.14.48",
    "rimraf": "^3.0.2"
  }
}
